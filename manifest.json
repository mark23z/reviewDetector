{
    "manifest_version": 3,
    "name": "Review Detector",
    "version": "0.0.1",
    "action": {
        "default_popup": "file.html",
        "default_title": "Review Detector"
    },
    "description": "Check Amazon reviews for authenticity.",
    "icons": {
        "16": "icons/bild16.png"
       
    },

    "background": {
        "service_worker": "background.js"
      },

      "permissions": [
        "tabs"
      ]
}